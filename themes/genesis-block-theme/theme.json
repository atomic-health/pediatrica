{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1366px",
			"wideSize": "100%",
			"allowCustomContentAndWideSize": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"units": ["px", "vh", "vw", "%"]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"palette": [
				{
					"name": "Transparent",
					"slug": "transparent",
					"color": "transparent"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#ffffff"
				},
				{
					"name": "Black",
					"slug": "black",
					"color": "#000000"
				},
				{
					"name": "Light Gray",
					"slug": "light-gray",
					"color": "#FCFAF2"
				},
				{
					"name": "Gray",
					"slug": "gray",
					"color": "#F2F0E7"
				},
				{
					"name": "Dark Gray",
					"slug": "dark-gray",
					"color": "#262626"
				},
				{
					"name": "Cyan",
					"slug": "cyan",
					"color": "#00667C"
				},
				{
					"name": "Blue",
					"slug": "blue",
					"color": "#293F76"
				},
				{
					"name": "Light Blue",
					"slug": "light-blue",
					"color": "#D9DEEF"	
				},
				{
					"name": "Purple",
					"slug": "purple",
					"color": "#8B3B70"
				},
				{
					"name": "Yellow",
					"slug": "yellow",
					"color": "#EBA607"
				}
			],
			"defaultPalette": false,
			"defaultGradients": false
		},
		"typography": {
			"lineHeight": true,
			"fontFamilies": [
				{
					"fontFamily": "'Museo Slab 500', sans-serif",
					"name": "Museo Slab 500",
					"slug": "museo-slab-500"
				},
				{
					"fontFamily": "'Proxima Nova', serif",
					"name": "Proxima Nova",
					"slug": "proxima-nova"
				},
				{
					"fontFamily": "'Neutraface Text', serif",
					"name": "Neutraface Text",
					"slug": "neutraface-text"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Box",
					"slug": "box",
					"shadow": "0px 4px 64px 0px rgba(0, 0, 0, 0.08)"
				},
				{
					"name": "Box no bottom",
					"slug": "box-no-bottom",
					"shadow": "0px -15px 64px -15px rgba(0, 0, 0, 0.08);"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--blue)"
				}
			},
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--blue)"
				}
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--purple)"
				}
			}
		}
	}
}
